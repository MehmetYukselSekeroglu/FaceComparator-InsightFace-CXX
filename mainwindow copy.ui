   <string notr="true">/* --- Genel Stil --- */
QWidget {
    color: #F0F0F0;
    background-color: #2D2D2D;
    font-family: &quot;Segoe UI&quot;, Arial, sans-serif;
}

QMainWindow {
    background-color: #2D2D2D;
}

/* --- Tab Widget Stili --- */
QTabWidget::pane {
    border: 1px solid #444;
    border-top: 0px;
    background-color: #3C3C3C;
}

QTabWidget::tab-bar {
    left: 10px; /* Tabların soldan başlaması için */
}

QTabBar::tab {
    background-color: #2D2D2D;
    color: #B0B0B0;
    border: 1px solid #444;
    border-bottom: none;
    padding: 10px 25px;
    font-weight: bold;
    min-width: 100px;
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
    margin-right: 2px;
}

QTabBar::tab:selected {
    background-color: #3C3C3C; /* Pane rengi ile aynı */
    color: #FFFFFF;
    border-bottom: 2px solid #0078D7; /* Seçili tab için alt çizgi */
}

QTabBar::tab:!selected:hover {
    background-color: #383838;
    color: #E0E0E0;
}

/* --- Buton Stili --- */
QPushButton {
    background-color: #0078D7;
    color: #FFFFFF;
    border: none;
    padding: 10px 20px;
    font-weight: bold;
    border-radius: 5px;
    min-height: 20px;
}

QPushButton:hover {
    background-color: #008CFF;
}

QPushButton:pressed {
    background-color: #006ABC;
}

QPushButton:disabled {
    background-color: #555;
    color: #888;
}

/* --- TextBrowser / Log Alanı --- */
QTextBrowser, QPlainTextEdit {
    background-color: #252525;
    border: 1px solid #444;
    color: #E0E0E0;
    border-radius: 5px;
    padding: 5px;
}

/* --- Resim Görüntüleme Alanı --- */
QLabel#label_image_tab_1, 
QLabel#label_source_face_tab_2, 
QLabel#label_target_face_tab_2 {
    background-color: #252525;
    border: 2px dashed #555;
    color: #777;
    font-weight: bold;
    border-radius: 5px;
}

/* --- Progress Bar Stili --- */
QProgressBar {
    border: 1px solid #555;
    border-radius: 5px;
    background-color: #3C3C3C;
    text-align: center;
    color: #F0F0F0;
    font-weight: bold;
}

QProgressBar::chunk {
    background-color: #0078D7;
    border-radius: 4px;
    margin: 1px;
}

/* --- Splitter Stili --- */
QSplitter::handle {
    background-color: #444;
}

QSplitter::handle:horizontal {
    width: 5px;
}

QSplitter::handle:vertical {
    height: 5px;
}
</string>
